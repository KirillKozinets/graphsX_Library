<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="PieChart">
        <attr name="startAngle" format="integer" />
        <attr name="descriptionColor" format="color" />
        <attr name="distanceDescription" format="float" />
        <attr name="descriptionTextSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="BaseCoordinateSystem">
        <attr name="colorAbscissaAxis" format="color" />
        <attr name="colorOrdinateAxis" format="color" />
        <attr name="colorDivisionAbscissaAxis" format="color" />
        <attr name="colorDivisionOrdinateAxis" format="color" />
        <attr name="showDivisionAbscissaAxis" format="boolean" />
        <attr name="showDivisionOrdinateAxis" format="boolean" />
        <attr name="divisionLineLengthTheAxis" format="integer" />
        <attr name="divisionLineThicknessTheAxis" format="integer" />
        <attr name="stepDivisionsAbscissaAxis" format="integer" />
        <attr name="stepDivisionsOrdinateAxis" format="integer" />
        <attr name="lineThicknessAbscissa" format="integer" />
        <attr name="lineThicknessOrdinate" format="integer" />
        <attr name="abscissaAxisShiftUp" format="integer" />
        <attr name="ordinateAxisShiftRight" format="integer" />
        <attr name="isAbscissaInCenter" format="boolean" />
        <attr name="isOrdinateInCenter" format="boolean" />
</declare-styleable>

    <declare-styleable name="LineGraph" parent="BaseCoordinateSystem">
        <attr name="isDivisionDescriptionAxisX" format="boolean" />
        <attr name="isDivisionDescriptionAxisY" format="boolean" />
        <attr name="isShowCoordinateGrid" format="boolean" />
        <attr name="scaleDivisionDescriptionAxisX" format="float" />
        <attr name="scaleDivisionDescriptionAxisY" format="float" />
        <attr name="colorDivisionDescriptionAxisX" format="color" />
        <attr name="colorDivisionDescriptionAxisY" format="color" />
        <attr name="colorCoordinateGrid" format="color" />
        <attr name="divisionDescriptionSize" format="dimension" />
        <attr name="xOffsetDescriptionDivision" format="integer" />
        <attr name="yOffsetDescriptionDivision" format="integer" />
        <attr name="thicknessCoordinateGrid" format="integer" />
    </declare-styleable>

</resources>